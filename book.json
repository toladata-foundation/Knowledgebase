{
    "gitbook": ">=3.2.0",
    "plugins": [
        "-collapsible-menu",
        "localized-footer",
        "search",
        "links",
        "expandable-chapters",
        "youtubex",
        "-downloadpdf",
        "advanced-emoji",
        "-sharing",
        "ga",
        "-gtm",
        "richquotes",
        "-sidebar-ad",
        "-edit-links",
        "anchors",
        "header",
        "meta"
        ],    
    "pluginsConfig":{
           "layout": {
                "headerPath" : "_layouts/header.md"
           },
           "richquotes": {
                "default": false
           },
           "language-picker": {
                "grid-columns": 3
           },
           "ga": {
                "token": "UA-109670551-1",
                "configuration": {
                     "cookieName": "TDHelpCookie",
                     "cookieDomain": "https://help.toladata.com"
                 }    
           },
           "gtm": {
                "token": "GTM-MZGX5NJ",
                "virtualPageViews": true
           },
           "meta": {
                "name": "google-site-verification",
                "content": "QHbZ-5LJ-NBS2JQ7d1EBhKvmwZGrrXLU-ZrXF0LjGBs"
           },
           "links": {
                "sidebar": {
                     "Contact Us": "mailto:support@toladata.com"
                     }
           },
           "github": {
                "url": "https://github.com/toladata"
           },
           "edit-link": {
                "base": "https://github.com/toladata/knowledgebase/edit/master/",
                "label": "Suggest Changes to this Page"
           },
           "localized-footer" : {
                "hline": true,
                "filename": "./FOOTER.md"
           },
           "sharing": {
                "facebook": false,
                "twitter": false,
                "google": false,
                "weibo": false,
                "instapaper": false,
                "vk": false,
                "all": []
           },
           "styles": {
                "website": "styles/website.css"
           },
           "downloadpdf": {
                "base": "https://www.gitbook.com/download/pdf/book/toladata/knowledgebase",
                "label": "Download PDF",
                "multilingual": false
           },
           "autocover": {
                "title": "TolaData Guide",
                "author": "November 2017",
                "font": {
                    "size": null,
                    "family": "Questrial",
                    "color": "#101F74"
                },
                "size": {
                    "w": 1800,
                    "h": 2360
                },
                "background": {
                    "color": "#09F"
                }
            }
    }
}

 		